#include "libft.h"
// #include <ctype.h>
// #include <stdio.h>

void	*ft_memchr(const void *s, int c, size_t n)
{
	unsigned char	*ptr_s;
	unsigned char	*ptr_c;
	size_t			i;

	ptr_c = (unsigned char *)&c;
	ptr_s = (unsigned char *)s;
	i = 0;
	while (i < n)
	{
		if (ptr_s[i] == *ptr_c)
			return (&ptr_s[i]);
		i ++;
	}
	return (NULL);
}

