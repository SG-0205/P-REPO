void	ft_putnbr(int n)
{
	long int	u_n;

	u_n = n;
	if (u_n < 0)
	{
		ft_putchar('-');
		u_n = -u_n;
	}
	if (u_n > 10)
	{
		ft_putnbr(u_n / 10);
		ft_putchar((u_n % 10) + '0');
	}
	else
		ft_putchar((u_n % 10) + '0');
}

