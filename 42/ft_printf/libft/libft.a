// #include <stdio.h>
// #include <stdlib.h>
#include "libft.h"

char	*ft_strnstr(const char *str, const char *to_find, size_t len)
{
	size_t	i;
	size_t	j;

	if (to_find == NULL || to_find[0] == 0)
		return ((char *)str);
	i = 0;
	j = 0;
	while (str[i])
	{
		j = 0;
		while (str[i] == to_find[j] && i <= len)
		{
			j ++;
			i ++;
			if (to_find[j] == '\0' || j == len)
				return ((char *)&str[i - j]);
		}
		i ++;
	}
	return (NULL);
}

