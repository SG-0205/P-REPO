char	*ft_strchr(const char *s, int c)
{
	int			i;
	int			total_len;
	const char	*c_is_here;

	total_len = 0;
	c_is_here = NULL;
	while (s[total_len])
		total_len ++;
	total_len += 1;
	i = 0;
	while (i <= total_len)
	{
		if (s[i] == c)
			c_is_here = &s[i];
		i ++;
	}
	return ((char *)c_is_here);
}

