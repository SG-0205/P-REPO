#include "libft.h"
// #include <stdio.h>

void	ft_intlen(long int n, size_t *container)
{
	unsigned int	ln;

	ln = n;
	if (n < 0)
	{
		*container += 1;
		ln = -n;
	}
	if (ln > 9)
	{
		*container += 1;
		ft_intlen(ln / 10, container);
	}
	else
		*container += 1;
}

