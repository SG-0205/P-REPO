char	*ft_strmap(char const *s, char (*f)(char))
{
	size_t	i;
	size_t	len;
	char	*new_str;

	if (!s || !f)
		return (NULL);
	len = ft_strlen((char *)s);
	i = 0;
	new_str = (char *)malloc(len * sizeof(char) + 1);
	while (s[i])
	{
		new_str[i] = f(s[i]);
		i ++;
	}
	new_str[i] = 0;
	return (new_str);
}

