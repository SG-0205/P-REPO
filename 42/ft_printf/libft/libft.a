// #include <stdio.h>
#include "libft.h"

char	*ft_strncat(char *dest, const char *src, size_t sz)
{
	size_t	i;
	size_t	j;

	j = 0;
	i = 0;
	while (dest[i])
		i ++;
	while (src[j] && j < sz)
	{
		dest[i] = src[j];
		i ++;
		j ++;
	}
	while (j < sz)
		dest[i++] = '\0';
	return (dest);
}

