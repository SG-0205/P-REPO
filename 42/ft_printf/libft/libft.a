// #include <stdio.h>
#include "libft.h"

void	*ft_memmove(void *dst, const void *src, size_t n)
{
	char		*ptr_dst;
	const char	*ptr_src;
	char		*ptr_swap;

	ptr_src = (const char *)src;
	ptr_dst = (char *)dst;
	ptr_swap = (char *) malloc((n) * sizeof(char));
	ft_bzero(ptr_swap, n);
	ft_memcpy(ptr_swap, ptr_src, n);
	ft_memcpy(ptr_dst, ptr_swap, n);
	free(ptr_swap);
	return (dst);
}

