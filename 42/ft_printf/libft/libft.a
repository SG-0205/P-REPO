#include <stdlib.h>
#include "libft.h"

char	*ft_strdup(const char *s)
{
	int		size;
	char	*new;

	new = NULL;
	size = 0;
	while (s[size])
		size ++;
	new = (char *)malloc(size * sizeof(char) + 1);
	if (!new)
		return (NULL);
	ft_strcpy(new, s);
	return (new);
}

