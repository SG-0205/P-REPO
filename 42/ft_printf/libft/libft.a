char	*ft_strnew(size_t size)
{
	char	*strnew;

	strnew = (char *)malloc(size * sizeof(char) + 1);
	if (!strnew)
		return (NULL);
	ft_bzero(strnew, size);
	strnew[size] = '\0';
	return (strnew);
}

