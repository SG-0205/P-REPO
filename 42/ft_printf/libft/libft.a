#include "libft.h"

char	*ft_strstr(const char *haystack, const char *needle)
{
	int		i;
	int		j;
	char	*ptr_needle;

	ptr_needle = (char *)haystack;
	if (needle == NULL || needle[0] == 0)
		return (ptr_needle);
	i = 0;
	j = 0;
	while (haystack[i])
	{
		j = 0;
		while (haystack[i] == needle[j])
		{
			j ++;
			i ++;
			if (needle[j] == '\0')
			{
				ptr_needle = (char *)&haystack[i - j];
				return (ptr_needle);
			}
		}
		i ++;
	}
	return (NULL);
}

