#include "libft.h"
// #include <ctype.h>
// #include <stdio.h>

int	ft_memcmp(const void *s1, const void *s2, size_t n)
{
	unsigned char	*ptr_s1;
	unsigned char	*ptr_s2;
	size_t			i;

	if (n == 0)
		return (0);
	i = 0;
	ptr_s1 = (unsigned char *)s1;
	ptr_s2 = (unsigned char *)s2;
	while (i < n - 1 && ptr_s1[i] == ptr_s2[i])
		i ++;
	return (ptr_s1[i] - ptr_s2[i]);
}

